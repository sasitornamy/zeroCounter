<!DOCTYPE html>
<html>
<head>
  <title>ZeroCounter</title>
  <%= stylesheet_link_tag :application, media: "all" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |key, value| %>
    <div class="flash-<%= key %>"><%= value %></div>
  <% end %>
  <%= yield %>
  <%= javascript_include_tag :application %>
</body>
</html>
